{
  "name": "uws-functional",
  "version": "1.0.9",
  "description": "Functional toolkit",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepublish": "concurrently -r \"git clean -d -f\" \"npm run test\"",
    "version": "concurrently -r \"npm run build\" \"git add -A dist\"",
    "postversion": "concurrently -r \"git push\" \"git push --tags\"",
    "bundle": "browserify index.js -d -t [ babelify --sourceMapRelative . --presets [ es2015 ] ] -p [ minifyify --map uws-functional.map --output build/uws-functional.map] > build/uws-functional.min.js",
    "test": "babel-tape-runner test/*.js | tap-spec",
    "createdist": "node node_modules/mkdirp/bin/cmd.js dist",
    "build": "concurrently -r \"npm run createdist\" \"npm run bundle\"",
    "watch": "watchify index.js -d -t [ babelify --sourceMapRelative . --presets [ es2015 ] ] -p [ minifyify --map uws-functional.map --output dist/uws-functional.map] -o dist/uws-functional.min.js"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-runtime": "^6.6.1",
    "babel-tape-runner": "^2.0.1",
    "babelify": "^7.2.0",
    "blue-tape": "^0.2.0",
    "browserify": "^13.0.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.3",
    "minifyify": "^7.3.2",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andywillis/uws-functional.git"
  },
  "keywords": [
    "functional",
    "toolkit"
  ],
  "author": "Andy Willis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andywillis/uws-functional/issues"
  },
  "homepage": "https://github.com/andywillis/uws-functional#readme",
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
